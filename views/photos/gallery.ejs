<% include ../include/header.ejs %>
<!--Breadcrumb Banner Area Start-->
<div class="breadcrumb-banner-area gallery body-top" style="background: url('/img/gallery_back.png')">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="breadcrumb-text">
                    <h1 class="text-center">Gallery</h1>
                    <div class="breadcrumb-bar">
                        <ul class="breadcrumb">
                            <li><a href="/">Home</a></li>
                            <li>Gallery</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--End of Breadcrumb Banner Area-->
<!--Gallery Area Start-->
<div class="gallery-area section-padding gallery-full-width">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="filter-menu">
                    <ul>
                        <li class="filter" data-filter="all">All</li>
                        <%categories.forEach(function(category){%>
                        <li class="filter" data-filter=".<%=category.DISTINCT%>"><%=category.DISTINCT%></li>
                        <%})%>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="filter-items">
                <%galleryList.forEach(function (gallery){%>
                <div class="mix single-items <%=gallery.category%> col-md-4 col-sm-6 col-xs-12 overlay-hover gallery-items">
                    <div class="overlay-effect">
                        <a><img src="<%=gallery.img%>" alt=""></a>
                        <div class="gallery-hover-effect">
                            <a class="gallery-icon venobox" href="<%=gallery.img%>"><i class="fa fa-image"></i></a>
                            <span class="gallery-text"><%=gallery.comment%></span>
                        </div>
                    </div>
                </div>
                <%})%>
            </div>
        </div>
    </div>
</div>
<!--End of Gallery Area-->
<% include ../include/footer.ejs %>